class A {
    String name;
    int rollno;
    void displayname(String name){
        System.out.println("hi! "+ name +"\n");
    }
}
class B extends A {
    void displayrollno(int rollno){
        System.out.println("Your rollno is "+ rollno);
    }
}

class Main {
    public static void main(String[] args) {
        B b = new B();
        b.displayname("Aditya");
        b.displayrollno(25584);
    }
}
